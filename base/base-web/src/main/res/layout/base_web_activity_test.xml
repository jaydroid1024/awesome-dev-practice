<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".web.TestActivity">

    <EditText
        android:id="@+id/et_web_first"
        android:layout_width="match_parent"
        android:hint="Web测试地址一"
        android:layout_height="100dp" />

    <Button
        android:id="@+id/btn_web_first"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Web测试地址一" />

    <EditText
        android:id="@+id/et_web_sec"
        android:hint="Web测试地址二"
        android:layout_width="match_parent"
        android:layout_height="100dp" />

    <Button
        android:id="@+id/btn_web_sec"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Web测试地址二" />


</LinearLayout>